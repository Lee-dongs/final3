<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="scheduleMapper">
	<resultMap type="schedule" id="deptScheduleResultSet">
		<result column="DEPT_SCHEDULE_NO" property="deptSchedule_No"/>
		<result column="DEPT_CODE" property="deptCode"/>
		<result column="JOB_CODE" property="jobCode"/>
		<result column="SCHEDULE_TITLE" property="scheduleTitle"/>
		<result column="SCHEDULE_CONTENT" property="scheduleContent"/>
		<result column="ENROLL_DATE" property="enrollDate"/>
		<result column="MODIFY_DATE" property="modifyDate"/>
		<result column="START_DATE" property="startDate"/>
		<result column="END_DATE" property="endDate"/>
		<result column="STATUS" property="status"/>
	</resultMap>

	<!-- 부서 스케줄 등록 메소드 -->
	<insert id="addSchedule">
		INSERT INTO DEPT_SCHEDULE (DEPT_SCHEDULE_NO,DEPT_CODE,JOB_CODE,SCHEDULE_TITLE,SCHEDULE_CONTENT,START_DATE,END_DATE)
          VALUES(SEQ_DEPT_SE.NEXTVAL,#{deptCode},#{jobCode},#{scheduleTitle},#{scheduleContent},#{startDate},#{endDate});
	</insert>
	
</mapper>